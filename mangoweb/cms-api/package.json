{
	"name": "mangoweb-cms-api",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"build": "tsc --build",
		"start": "dotenv -- nodemon --watch dist --ext js --delay 500ms ./node_modules/.bin/console start",
		"console": "dotenv -- console",
		"reset": "dotenv -- console drop && dotenv -- console engine:migrations:continue && dotenv -- console init",
		"sync": "dotenv -- console engine:migrations:continue && dotenv -- console init"
	},
	"dependencies": {
		"cms-api": "^0.0.0",
		"cms-common": "^0.0.0",
		"knex": "^0.16.2",
		"pg": "^7.4.3"
	},
	"devDependencies": {
		"@types/knex": "^0.14.16",
		"dotenv-cli": "^1.4.0",
		"nodemon": "^1.18.3",
		"tslint": "^5.10.0",
		"typescript": "^3.2.2"
	}
}
