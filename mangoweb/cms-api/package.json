{
	"name": "mangoweb-cms-api",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"build": "tsc --build",
		"start": "dotenv -- nodemon --watch dist --ext js --delay 500ms --inspect=0.0.0.0 ./dist/src/index.js start",
		"console": "dotenv -- node ./dist/src/index.js",
		"reset": "dotenv --  node ./dist/src/index.js drop && dotenv --  node ./dist/src/index.js update",
		"sync": "dotenv --  node ./dist/src/index.js update"
	},
	"dependencies": {
		"cms-api": "^0.0.0",
		"cms-common": "^0.0.0"
	},
	"devDependencies": {
		"dotenv-cli": "^1.4.0",
		"nodemon": "^1.18.3",
		"tslint": "^5.10.0",
		"typescript": "^3.5.2"
	}
}
