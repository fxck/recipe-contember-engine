{
  "//": [
    "graphql-code-generator is locked on 0.10.5 because of bc break https://github.com/dotansimha/graphql-code-generator/pull/444/files#r207878190"
  ],
  "name": "cms-api",
  "version": "0.0.0",
  "private": true,
  "main": "dist/src/index.js",
  "typings": "dist/src/index.d.ts",
  "scripts": {
    "prebuild": "env CODEGEN_IMMUTABLE_TYPES=1 npx gql-gen --require ts-node/register --schema ./src/tenant-api/schema/tenant.graphql.ts --template typescript --skip-documents --out ./src/tenant-api/schema/types.ts",
    "build": "tsc --build --verbose",
    "test": "mocha --ui bdd ./dist/tests/cases/**/*.js",
    "test:debug": "mocha --inspect --ui bdd"
  },
  "dependencies": {
    "apollo-server-express": "^2.0.0",
    "cms-common": "0.0.0",
    "express": "^4.16.3",
    "bcrypt": "^2.0.0",
    "graphql": "^0.13.2",
    "graphql-code-generator": "0.10.5",
    "graphql-codegen-typescript-template": "0.10.5",
    "graphql-errors": "^2.1.0",
    "join-monster": "^2.0.16",
    "knex": "^0.14.6",
    "node-pg-migrate": "^3.5.1",
    "pg": "^7.4.3",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/bluebird": "3.5.22",
    "@types/bcrypt": "^2.0.0",
    "@types/chai": "^4.1.4",
    "@types/dotenv": "^4.0.3",
    "@types/knex": "^0.14.16",
    "@types/mocha": "^5.2.4",
    "@types/mock-knex": "^0.3.2",
    "@types/sinon": "^5.0.1",
    "@types/uuid": "^3.4.3",
    "@types/ws": "^5.1.2",
    "chai": "^4.1.2",
    "concurrently": "^3.6.0",
    "dotenv": "^6.0.0",
    "mocha": "^5.2.0",
    "mock-knex": "^0.4.0",
    "sinon": "^6.1.3",
    "ts-node": "^7.0.0",
    "tslint": "^5.10.0",
    "typescript": "^3.0.1"
  }
}
